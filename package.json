{
  "name": "yollama",
  "version": "1.0.0",
  "description": "A front-end library for accessing your locally or external Ollama API!",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "buildwatch": "tsc -p tsconfig.esm.json --watch",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevInsilicon/YOllama.git"
  },
  "keywords": [
    "YOllama",
    "Ollama",
    "Library",
    "NodeJS"
  ],
  "author": "insilicon",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DevInsilicon/YOllama/issues"
  },
  "homepage": "https://github.com/DevInsilicon/YOllama#readme",
  "devDependencies": {
    "@types/node": "^22.7.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}